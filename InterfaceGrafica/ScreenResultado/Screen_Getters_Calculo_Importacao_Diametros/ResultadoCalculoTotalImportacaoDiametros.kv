<ResultadoCalculoTotalImportacaoDiametros>:
    on_enter: root.Calcular()
    FloatLayout:
        id: local_tabela
        LabelConfig:
            text: 'Resultado Cálculo do deslocamento total na ponta do eixo-árvore com importação dos diâmetros'
            pos_hint: {'center_x': 0.5, 'top': 1.}

        BotaoVoltar:
            pos_hint: {'center_x': 0.9, 'y': 0.05}
            on_release:
                app.get_running_app().root.current = 'CalculoTotalImportacaoDiametros'
                root.Limpar()

